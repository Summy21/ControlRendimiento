<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.summy.controlrendimiento">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RegistroActivity" />
        <activity android:name=".DiariaActivity" />
        <activity android:name=".RegistroAdminActivity" />

        <!-- aqui habilitamos el boton volver de activity dtos frecuencia y volumen -->
        <activity
            android:name=".DatosFrecuenciaVolumenActivity"
            android:label="@string/title_datos_frecuencia_volumen"
            android:parentActivityName=".DiariaActivity">

            <!-- Este meta-data nos permite saber cual sera la actividad padre de esta actividad -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DiariaActivity" />
        </activity>
        <!-- ////////////////////////////////////////////////////////////////// -->

        <activity android:name=".ObtenerCompetenciasApiActivity" />
        <activity android:name=".PeriodosActivity" />
        <activity android:name=".DialogCompetenciaNacionalActivity" />
    </application>

</manifest>